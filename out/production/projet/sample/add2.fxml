<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="210.0" prefWidth="325.0" xmlns="http://javafx.com/javafx/9" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.add2controller">
    <children>
        <HBox prefHeight="150.0" prefWidth="325.0">
            <children>
                <VBox prefHeight="162.0" prefWidth="61.0" spacing="32.0">
                    <children>
                        <Label alignment="CENTER" text="Devision" />
                        <Label alignment="CENTER" layoutX="10.0" layoutY="10.0" text="Service" />
                    </children>
                </VBox>
                <VBox prefHeight="125.0" prefWidth="176.0" spacing="20.0">
                    <padding>
                        <Insets left="20.0" />
                    </padding>
                    <children>
                        <ComboBox fx:id="div" onAction="#divv" prefWidth="150.0" />
                        <ComboBox fx:id="serv" prefWidth="150.0" />
                <Button fx:id="load" mnemonicParsing="false" onAction="#load" text="Image" />
                    </children>
                </VBox>
            </children>
            <padding>
                <Insets bottom="10.0" left="50.0" right="20.0" top="20.0" />
            </padding>
        </HBox>
      <Label fx:id="path" alignment="CENTER" contentDisplay="CENTER" prefHeight="16.0" prefWidth="325.0" />
        <HBox prefHeight="37.0" prefWidth="200.0" spacing="30.0">
            <children>
                <Button fx:id="ajout" mnemonicParsing="false" onAction="#ajout" text="Ajouter" />
                <Button fx:id="close" mnemonicParsing="false" onAction="#close" text="Annuler" />
            </children>
            <padding>
                <Insets bottom="20.0" left="90.0" right="20.0" top="10.0" />
            </padding>
        </HBox>
    </children>
</VBox>
