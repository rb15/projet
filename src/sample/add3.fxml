<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="179.0" prefWidth="325.0" xmlns="http://javafx.com/javafx/9" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.add3controller">
    <children>
        <HBox prefHeight="41.0" prefWidth="325.0">
            <children>
                <VBox prefHeight="74.0" prefWidth="61.0" spacing="32.0">
                    <children>
                        <Label alignment="CENTER" text="Devision" />
                        <Label alignment="CENTER" layoutX="10.0" layoutY="10.0" text="Service" />
                    </children>
                </VBox>
                <VBox prefHeight="205.0" prefWidth="176.0" spacing="20.0">
                    <padding>
                        <Insets left="20.0" />
                    </padding>
                    <children>
                        <ComboBox fx:id="div"  prefWidth="150.0" />
                  <TextField fx:id="serv" />
                    </children>
                </VBox>
            </children>
            <padding>
                <Insets bottom="10.0" left="50.0" right="20.0" top="20.0" />
            </padding>
        </HBox>
        <HBox prefHeight="44.0" prefWidth="325.0" spacing="30.0">
            <children>
                <Button fx:id="ajout" mnemonicParsing="false" onAction="#ajout" text="Ajouter" />
                <Button fx:id="close" mnemonicParsing="false" onAction="#close" text="Annuler" />
            </children>
            <padding>
                <Insets bottom="20.0" left="90.0" right="20.0" top="10.0" />
            </padding>
        </HBox>
    </children>
</VBox>
